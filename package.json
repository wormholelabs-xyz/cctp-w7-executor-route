{
  "name": "@wormhole-labs/cctp-executor-route",
  "version": "0.0.1-beta.11",
  "license": "Apache-2.0",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "description": "This package implements an AutomaticRoute for use with the Wormhole Typescript SDK",
  "files": [
    "dist"
  ],
  "keywords": [
    "wormhole",
    "sdk",
    "typescript",
    "connect"
  ],
  "engines": {
    "node": ">=16"
  },
  "sideEffects": true,
  "scripts": {
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "build": "npm run build:cjs && npm run build:esm",
    "examples:transfer": "npx tsx examples/transfer.ts",
    "test": "vitest"
  },
  "peerDependencies": {
    "@wormhole-foundation/sdk-aptos": "^1.22.0",
    "@wormhole-foundation/sdk-connect": "^1.22.0",
    "@wormhole-foundation/sdk-evm": "^1.22.0",
    "@wormhole-foundation/sdk-solana": "^1.22.0",
    "@wormhole-foundation/sdk-solana-cctp": "^1.22.0",
    "@wormhole-foundation/sdk-sui": "^1.22.0",
    "@wormhole-foundation/sdk-sui-cctp": "^1.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.6",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^3.1.1"
  },
  "dependencies": {
    "@aptos-labs/ts-sdk": "^2.0.0",
    "@coral-xyz/anchor": "^0.29.0",
    "@mysten/sui": "^1.21.2",
    "@solana/spl-token": "^0.3.9",
    "@solana/web3.js": "^1.95.8",
    "binary-layout": "^1.2.0"
  }
}
